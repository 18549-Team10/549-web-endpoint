               function findValues() {
           var myList;
           $.getJSON('/json/currentFill.json').done(function (data) {
               myList = data;
               var jsonData = document.getElementById("json-data");
               jsonData.innerHTML += "Container ID = ";
               jsonData.innerHTML += data.containerID;
               jsonData.innerHTML += "<br>";
               jsonData.innerHTML += "Fill Level =";
               jsonData.innerHTML += data.fillLevel;
               jsonData.innerHTML += "<br>";
               console.log(data);
           });
       }

        function getJSONFile() {
            var myList;
            $.getJSON('/json/currentFill.json').done(function (data) {
                myList = data;
                var jsonData = document.getElementById("json-data");
                jsonData.innerHTML += "Container ID = ";
                jsonData.innerHTML += data.containerID;
                jsonData.innerHTML += "<br>";
                jsonData.innerHTML += "Fill Level = ";
                jsonData.innerHTML += data.fillLevel;
                jsonData.innerHTML += "<br>";
                var lastModified = document.getElementById("current-time");
                lastModified.innerHTML += data.currentTime;
            });

        }
    </script>